#general stuff
alias chomp="perl -pi -e 'chomp if eof'"
alias vi="vim"
alias now="date +'%Y-%m-%dT%H:%M:%SZ'"

#svn stuff
alias ss="svn status"
alias scm="svn commit -m"
alias sup="svn update"

#git
alias gst='git status'
alias gaa='git add -A'
alias gcm='git commit -m'
alias gpl='git pull'
alias gps='git push'
alias gdf='git diff'
alias glg='git log'
alias gco='git checkout'
alias gbr='git branch'
alias gmg='git merge'
